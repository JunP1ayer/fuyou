{
  "buildCommand": "",
  "outputDirectory": ".",
  "installCommand": "",
  "functions": {
    "api/openai-vision.js": {
      "maxDuration": 30
    },
    "api/gemini-vision.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/openai-vision",
      "destination": "/api/openai-vision.js"
    },
    {
      "source": "/api/gemini-vision", 
      "destination": "/api/gemini-vision.js"
    }
  ]
}